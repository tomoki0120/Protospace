.jumbotron
  .container.text-center
    = image_tag("header_logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height: "73px")

.filter-nav
  .container
    .row
      %ul.nav.nav-pills.col-lg-11
        %li.active{role: "presentation"}
          =link_to 'Popular PROTO',populars_path
        %li.active{role: "presentation"}
          =link_to 'Newest PROTO',newests_path
      = button_tag class: "btn btn-default col-lg-1" do
        = link_to 'view tag',tags_path

.container.proto-list
  .row
    =render partial: 'prototypes/prototype', collection: @prototypes
.text-center
  %ul.pagination
    =paginate(@prototypes)
