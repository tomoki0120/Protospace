.col-sm-4.col-md-3.proto-content
  .thumbnail
    =link_to image_tag(prototype.image_main_photo), prototype_path(prototype)
    .caption
      %h3
        =prototype.title
      .proto-meta
        .proto-user
          =link_to "#{prototype.user.name}",user_path(prototype.user)
        .proto-posted
          = prototype.created_at.strftime("%Y/%m/%d(#{%w(日 月 火 水 木 金 土)[prototype.created_at.wday]})")
      %ul.proto-tag-list.list-inline
        - prototype.tags.each do |tag|
          %li
            =link_to tag.name,tag_path(tag.id),class: "btn btn-default"

