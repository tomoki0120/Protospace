.container.user-page
  %header.user-nav
    .media
      .media-left
        %a{href: "#"}
          %img.media-object{alt: "64x64", "data-holder-rendered": "true", "data-src": "holder.js/64x64", src: "data:image/svg+xml;base64,", style: "width: 64px; height: 64px;"}/
      .media-body
        %h4#top-aligned-media.media-heading
          = @user.name
          %span.label.label-default.project-number
            29
            %i Projects
        .proto-user
          %span.degree 
            = @user.position
        .row
          .col-md-6
            %h4 Profile
            %p 
              =@user.profile
          .col-md-6
            %h4 Works
            %p 
            =@user.occupation
.container.proto-list
  .row
    =render partial: 'prototypes/prototype', collection: @prototypes
